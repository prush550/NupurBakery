# MongoDB Atlas Connection String
# Get this from: https://cloud.mongodb.com/ -> Connect -> Drivers
MONGODB_URI=mongodb+srv://username:password@cluster.xxxxx.mongodb.net/?retryWrites=true&w=majority

# Cloudinary Configuration
# Get these from: https://console.cloudinary.com/settings/api-keys
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Resend Email Service
# Get this from: https://resend.com/api-keys
RESEND_API_KEY=re_xxxxxxxxxxxx
